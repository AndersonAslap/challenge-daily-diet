{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "lib": ["es6", "dom", "webworker", "react", "react-native"],
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@routes/*": ["./src/routes/*"],
      "@screens/*": ["./src/screens/*"],
      "@storage/*": ["./src/storage/*"],
      "@utils/*": ["./src/utils/*"]
    }
  }
}
